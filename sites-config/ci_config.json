{
  "artifactStructure": {
    "assets": [
      {
        "root": "storybook-static"
      }
    ],
    "hbsTemplates": []
  },
  "dependencies": {
    "installDepsCmd": "echo $npm_config_auth && npm install",
    "requiredFiles": ["package.json", "pnpm-lock.json", ".npmrc"]
  },
  "buildArtifacts": {
    "buildSetupCmd": "npm ci",
    "buildCmd": "npm run build-storybook"
  },
  "livePreview": {
    "serveSetupCmd": "(npm run build-storybook)",
      "serveCmd": "(npm run storybook -- -p 8080)"
  }
}